<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="95dp"
    android:layout_height="70dp"
    android:background="#fff">

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/main_taobao_recyclerview_circleImageView"
        android:layout_width="32dp"
        android:layout_height="29dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.39" />

    <TextView
        android:id="@+id/main_taobao_recyclerview_textView7"
        android:layout_width="90dp"
        android:layout_height="17dp"
        android:textSize="12dp"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/main_taobao_recyclerview_circleImageView"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintStart_toStartOf="@+id/main_taobao_recyclerview_circleImageView"
        app:layout_constraintTop_toBottomOf="@+id/main_taobao_recyclerview_circleImageView"
        app:layout_constraintVertical_bias="0.0"
        tools:text="红包卡券" />

</androidx.constraintlayout.widget.ConstraintLayout>